#include <stdio.h>

int main(void) {
    double appleDensity;     // ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡πÅ‡∏ô‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏• (g/cm¬≥)
    double waterDensity;     // ‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡πÅ‡∏ô‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡∏ô‡πâ‡∏≥ (g/cm¬≥)
    double waterHeight = 0;  // ‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ô‡πâ‡∏≥‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô (cm)
    double step = 0.2;       // ‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥‡∏ó‡∏µ‡∏•‡∏∞ 2 ‡∏°‡∏¥‡∏•‡∏•‡∏¥‡πÄ‡∏°‡∏ï‡∏£ (0.2 cm)
    double radius;           // ‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Ç‡∏≠‡∏á‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏• (cm)
    int rounds = 0;          // ‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥

    printf("‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Ç‡∏≠‡∏á‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏• (cm): ");
    scanf("%lf", &radius);

    printf("‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡πÅ‡∏ô‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏• (g/cm^3): ");
    scanf("%lf", &appleDensity);

    printf("‡∏Ñ‡∏ß‡∏≤‡∏°‡∏´‡∏ô‡∏≤‡πÅ‡∏ô‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡∏ô‡πâ‡∏≥ (g/cm^3): ");
    scanf("%lf", &waterDensity);

    // ‡∏ñ‡πâ‡∏≤‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏•‡∏´‡∏ô‡∏±‡∏Å‡∏Å‡∏ß‡πà‡∏≤‡∏ô‡πâ‡∏≥ ‚Üí ‡∏à‡∏°‡πÅ‡∏ô‡πà‡∏ô‡∏≠‡∏ô
    if (appleDensity > waterDensity) {
        printf("‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏•‡∏´‡∏ô‡∏±‡∏Å‡∏Å‡∏ß‡πà‡∏≤‡∏ô‡πâ‡∏≥ ‡∏à‡∏°‡πÅ‡∏ô‡πà‡∏ô‡∏≠‡∏ô‡∏Ñ‡∏£‡∏±‡∏ö \n");
    } 
    else {
        printf("‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏à‡∏≥‡∏•‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥‡∏ó‡∏µ‡∏•‡∏∞ %.1f cm (2 ‡∏°‡∏°.)...\n", step);
        while (waterHeight <= 2 * radius) {
            rounds++; // ‡∏ô‡∏±‡∏ö‡∏à‡∏≥‡∏ô‡∏ß‡∏ô‡∏£‡∏≠‡∏ö‡∏ó‡∏µ‡πà‡πÄ‡∏ï‡∏¥‡∏°
            printf("‡∏£‡∏≠‡∏ö‡∏ó‡∏µ‡πà %d: ‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥‡∏à‡∏ô‡∏£‡∏∞‡∏î‡∏±‡∏ö %.1f cm\n", rounds, waterHeight);

            // ‡∏™‡∏°‡∏°‡∏ï‡∏¥‡∏ß‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡∏≠‡∏¢‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ô‡πâ‡∏≥‡∏ñ‡∏∂‡∏á‡∏Ñ‡∏£‡∏∂‡πà‡∏á‡∏•‡∏π‡∏Å
            if (waterHeight >= radius) {  
                printf("\nüçé ‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏•‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡∏≠‡∏¢‡∏ó‡∏µ‡πà‡∏£‡∏∞‡∏î‡∏±‡∏ö‡∏ô‡πâ‡∏≥ %.1f cm\n", waterHeight);
                printf("‡πÉ‡∏ä‡πâ‡πÄ‡∏ß‡∏•‡∏≤‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î %d ‡∏£‡∏≠‡∏ö\n", rounds);
                return 0;
            }
            waterHeight = waterHeight + step; // ‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥‡πÄ‡∏û‡∏¥‡πà‡∏°
        }
        printf("‡πÄ‡∏ï‡∏¥‡∏°‡∏ô‡πâ‡∏≥‡∏à‡∏ô‡∏ó‡πà‡∏ß‡∏°‡πÅ‡∏≠‡∏õ‡πÄ‡∏õ‡∏¥‡πâ‡∏•‡πÅ‡∏•‡πâ‡∏ß‡πÅ‡∏ï‡πà‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏•‡∏≠‡∏¢ \n");
    }

    return 0;
}



